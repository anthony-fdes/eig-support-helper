  {
    "name": "BrowserStack Automate Helper",
    "version": "1.0",
    "description": "Open a session's details.",
    "manifest_version": 2,
    "permissions": ["contextMenus"],
    "content_scripts": [{
      "js": [
        "publicLink.js"
      ],
      "matches": [
        "https://automate.browserstack.com/*",
        "https://app-automate.browserstack.com/*"
      ]
    }],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "browserstack.png"
    },
    "icons": {
      "16": "browserstack.png",
      "48": "browserstack.png",
      "128": "browserstack.png"
    }
  }
